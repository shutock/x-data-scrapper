{
  "name": "x-data-scrapper",
  "author": "shutock <yo@shutock.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun --bun src/index.ts",
    "dev": "bun --bun --watch src/index.ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "test": "bun test",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f x-data-scrapper",
    "docker:restart": "docker compose restart x-data-scrapper",
    "docker:ps": "docker compose ps",
    "docker:start": "docker compose up -d --build"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/bun": "^1.3.4",
    "@types/lodash": "^4.17.21",
    "prettier": "^3.7.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "bun": "^1.3.4",
    "cheerio": "^1.1.2",
    "hono": "^4.11.1",
    "lodash": "^4.17.21",
    "ora": "^9.0.0",
    "puppeteer": "^24.31.0",
    "puppeteer-cluster": "^0.25.0",
    "zod": "^4.1.13"
  }
}
